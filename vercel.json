{
  "version": 2,
  "builds": [
    { "src": "api/*.js", "use": "@vercel/node" },
    { "src": "web_dist/**", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "^/tasks(?:/.*)?$", "dest": "/api/index.js" },
    { "src": "^/$", "dest": "/web_dist/index.html" },
    { "src": "^(?!/api|/tasks)(.*)$", "dest": "/web_dist/$1" }
  ]
}
